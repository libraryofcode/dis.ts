{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "@dis.ts/eslint-config"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "ignorePatterns": ["node_modules/**", "*.js", "*.d.ts"],
    "rules": {}
}
